    var interpolator = 17
    print("Daniel Johnston made \(interpolator) studio albums.")

Challenge Temperature: https://www.codecademy.com/courses/learn-swift/lessons/swift-variables/exercises/challenge-temperature
  
    Variables Constants: https://www.codecademy.com/courses/learn-swift/lessons/swift-variables/exercises/constants
    var tempf: Double = 34
    var tempc: Double
    tempc = (tempf - 32) / 1.8
    print("The temp is /(tempc) degrees Celsius")

Challenge Body Mass Index: https://www.codecademy.com/courses/learn-swift/lessons/swift-variables/exercises/review
    
    var weight: Int = 185
    var height: Int = 66
    var bmi = weight * (height * height)
